// EX04 Shell & Variables

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh 'echo My Build Number is: ${BUILD_NUMBER}'
            }
        }
        stage('Stage Two') {
            steps {
USER=`echo ${BUILD_USER_ID} | sed 's/.*(\(.*\))/\1/'`
            }
        }

        }
    }
