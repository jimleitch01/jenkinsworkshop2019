// EX04 Shell & Variables

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh 'echo My Build Number is: ${BUILD_NUMBER}'
            }
        }
        stage('Step TWO'){
            steps{
                sh 'echo The branch is ${BRANCH_NAME}'
                sh 'echo The last commit was ${GIT_COMMIT}'
            }
        }
  

        }
    }
