// EX04 Shell & Variables

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh 'echo My Build Number is: ${BUILD_NUMBER}, BRANCH NAME is: ${BRANCH_NAME}, JOB NAME is: ${JOB_NAME}'
            }
            ,
            steps {
                sh 'Step 2 echo My Build Number is: ${BUILD_NUMBER}, BRANCH NAME is: ${BRANCH_NAME}, JOB NAME is: ${JOB_NAME}'
            }
        }

  

        }
    }
