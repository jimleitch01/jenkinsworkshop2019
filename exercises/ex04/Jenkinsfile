// EX04 Shell & Variables

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh 'echo My Build Number is: ${BUILD_NUMBER}'
            }
        }

        stage('Stage Two') {
            steps {
                sh 'echo My Build is based on : ${GIT_COMMIT}'
                sh 'env'
            }
        }
  

    }
}
