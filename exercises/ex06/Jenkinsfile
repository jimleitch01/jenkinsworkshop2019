// EX06 Parallel Running

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
            }
        }

    stage('run-parallel-branches') {
  steps {
    parallel(
      a: {
        echo "This is branch a"
      },
      b: {
        echo "This is branch b"
      }
    )
  }
}

    stage('run-parallel-branches') {
  steps {
    parallel(
      1: {
        echo "This is branch 1"
      },
      2: {
        echo "This is branch 2"
      }
    )
  }
}





        }
    }



 
