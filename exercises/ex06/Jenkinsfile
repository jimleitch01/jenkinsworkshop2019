// EX06 Parallel Running

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
            }
        }

    
    
    stage('run-parallel-branches woef') {
  steps {
    parallel(
      a: {
        echo "This is branch woef a"
      },
      b: {
        echo "This is branch woef b"
      }
    )
  }
}


        }
    }



 