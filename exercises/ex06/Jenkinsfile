// EX06 Parallel Running

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
            }
        }

    stage('run-parallel-branches') {
  steps {
    parallel(
      UnitTest: {
        echo "1. This is branch a"
        echo "2. This is branch a"
      },
      codeReview: {
        echo "This is branch b"
      },
      ApiTest: {
        echo "This is branch c"
      }
    )
  }
}


        }
    }



 