// EX06 Parallel Running

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
            }
        }

    stage('run-parallel-branches') {
  steps {
    parallel(
      1: {
        echo "This is branch a"
      },
      2: {
        echo "This is branch b"
      },
      3: {
        echo "This is branch 3"
      }
    )
  }
}


        }
    }



 