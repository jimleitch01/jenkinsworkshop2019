// EX06 Parallel Running

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
            }
        }

    stage('run-parallel-branches') {
  steps {
    parallel(
      a: {
        echo "This is branch a"
        parallel(
a1: {echo "a1"}, b2 :{echo "a2"}

        )
      },
      b: {
        echo "This is branch b"
      }
    )
  }
}


        }
    }



 