// EX06 Parallel Running

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
                sh "date -Ins"
            }
        }

    stage('run-parallel-branches') {
  steps {
    parallel(
      a: {
        echo "This is branch a"
        sh "date -Ins"
      },
      b: {
        echo "This is branch b"
        sh "date -Ins"
      }
    )
  }
}
        stage('Stage Three') {
            steps {
                sh '''
                    echo Stage Three
                '''
                sh "date -Ins"
            }
        }

        }
    }



 