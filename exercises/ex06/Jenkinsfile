// EX06 Parallel Running

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
            }
        }

    stage('run-parallel-branches') {
  steps {
    parallel(
      mac: {
        echo "This is branch mac"
      },
      win: {
        echo "This is branch win"
      },
      lin: {
        echo "This is branch lin"
      }
    )
  }
}


        }
    }



 