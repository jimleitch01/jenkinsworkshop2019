// EX05 Blocks

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Once upon a time,
                '''
            }
        }

        stage('Parallel lines') {
            parallel {
                stage('Stage Two') {
                    steps {
                        sh '''
                            echo in a galaxy
                        '''
                    }
                }

                stage('Stage Two.five') {
                    steps {
                        sh '''
                            echo in a land
                        '''
                    }
                }
            }
        }

        stage('Stage Three') {
            steps {
                sh '''
                    echo far far away
                '''
            }
        }

    }
}