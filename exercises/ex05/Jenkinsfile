// EX05 Blocks

pipeline {
    agent any

    stages {
        stage('Stage One') {
            steps {
                sh '''
                    echo Stage One
                '''
                sh 'echo Workspace: ${WORKSPACE}'
            }
        }

        stage('Stage Two') {
            steps {
                sh '''
                    echo Stage Two
                '''
                sh 'echo My Build Tag is: ${BUILD_TAG}'
            }
        }


        }
    }