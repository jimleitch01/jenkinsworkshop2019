// EX05 Blocks

pipeline {
    agent any

    stages {
        stage('Maandag') {
            steps {
                sh '''
                    echo Stage One
                '''
            }
        }

        stage('Dinsdag') {
            steps {
                sh '''
                    echo Stage Two, Sander
                '''
            }
        }

        stage('Woensdag, de week is doormidden') {
            steps {
                sh '''
                    echo Stage Two, Sander
                '''
            }
        }

        stage('Donderdag, bijna weekend') {
            steps {
                sh '''
                    echo Stage Two, Sander
                '''
            }
        }

        stage('Vrijdag, echt bijna weekend') {
            steps {
                sh '''
                    echo Stage Two, Sander
                '''
            }
        }

        }
    }